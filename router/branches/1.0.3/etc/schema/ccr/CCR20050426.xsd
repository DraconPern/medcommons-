	<!--These are the Various Expressions of Time in the CCR-->
	<xs:complexType name="DateTimeType">
		<xs:annotation>
			<xs:documentation>This is used for the expression of DateTime as exact or inexact.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Type" minOccurs="0"/>
			<xs:element name="ExactDateTime" type="ISO8601DateTime" minOccurs="0"/>
			<xs:element ref="Age" minOccurs="0"/>
			<xs:element name="ApproximateDateTime" type="CodedDescriptionType" minOccurs="0"/>
			<xs:element name="DateTimeRange" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BeginRange" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ExactDateTime" type="ISO8601DateTime" minOccurs="0"/>
									<xs:element ref="Age" minOccurs="0"/>
									<xs:element name="ApproximateDateTime" type="CodedDescriptionType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
						<xs:element name="EndRange" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ExactDateTime" type="ISO8601DateTime" minOccurs="0"/>
									<xs:element ref="Age" minOccurs="0"/>
									<xs:element name="ApproximateDateTime" type="CodedDescriptionType" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
