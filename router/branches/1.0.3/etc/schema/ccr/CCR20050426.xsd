	<xs:complexType name="ExactDateTimeType">
		<xs:annotation>
			<xs:documentation>This is used to express time as exact, but not necessarily down to the second.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="Type" minOccurs="0"/>
			<xs:element name="ExactDateTime" type="ISO8601DateTime" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="ISO8601DateTimeSeconds">
		<xs:annotation>
			<xs:documentation>
                This is a simple type that allows date times to be expressed as ISO-8601 date-time formats, 
                with precision of at least seconds.
            </xs:documentation>
		</xs:annotation>
		<xs:restriction base="ISO8601DateTime">
			<xs:pattern value="\d\d\d\d\-\d\d\-\d\dT\d\d:\d\d:\d\d(Z|([\-+])\d\d:\d\d)"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ISO8601DateTime">
		<xs:annotation>
			<xs:documentation>
                This is a simple type that allows date times to be expressed as ISO-8601 date-time formats, e.g.,
                CCYY[-MM[-DD[Thh[:mm[:ss]]]]Z
                CCYY[-MM[-DD[Thh[:mm[:ss]]]]+hh:mm
                CCYY[-MM[-DD[Thh[:mm[:ss]]]]-hh:mm
                
                All date times in the CCR must express a timezone offset, either using Z [universal coordinated 
                time, or Zulu time], or an offset in hours and minutes.  The CCR futher requires that the Timezone
                offset be a legal time zone.  This latter constraint cannot be expressed in the schema, as time 
                zones are determined by political entities [e.g., Nations or States].  There presently exist time
                zones in the form ##:15 and ##:30.
            </xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Every month except February can have 30 days</xs:documentation>
					</xs:annotation>
					<xs:pattern value="\d\d\d\d(\-(0[13-9]|1[012])(\-((0[1-9])|1\d|2\d|30)(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Jan, Mar, May, Jul, Aug, Oct, and Dec can have 31 days</xs:documentation>
					</xs:annotation>
					<xs:pattern value="\d\d\d\d(\-(0[13578]|1[02])(\-31(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Feb in any year can have 28 days</xs:documentation>
					</xs:annotation>
					<xs:pattern value="\d\d\d\d(\-02(\-((0[1-9])|1\d|2[0-8])(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Feb in years ending in 04 or 08 can have 29 days </xs:documentation>
					</xs:annotation>
					<xs:pattern value="\d\d[0][48](\-02(\-29(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Feb in years ending in 20, 24, 28, 40, 44, 48, 60, 64, 68, 80, 84 or 88 can have 29 days </xs:documentation>
					</xs:annotation>
					<xs:pattern value="\d\d[2468][48](\-02(\-29(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Feb in years ending in 12, 16, 32, 36, 52, 56, 72, 76, 92 or 96 can have 29 days </xs:documentation>
					</xs:annotation>
					<xs:pattern value="\d\d[13579][26](\-02(\-29(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Feb in Centuries divisible by 4 can have 29 days</xs:documentation>
					</xs:annotation>
					<xs:pattern value="[02468][048]00(\-02(\-29(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:annotation>
						<xs:documentation>Feb in Centuries divisible by 4 can have 29 days</xs:documentation>
					</xs:annotation>
					<xs:pattern value="[13579][26]00(\-02(\-29(T(0\d|1\d|2[0-3])(:[0-5]\d){0,2})?)?)?(Z|(\-|\+)(0\d|1\d|2[0-3])(:[0-5]\d))"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
