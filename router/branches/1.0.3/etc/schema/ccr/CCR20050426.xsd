	<!-- Reusable Global Elements -->
	<xs:complexType name="CCRCodedDataObjectType">
		<xs:annotation>
			<xs:documentation>This defines a Base Type used to capture: a) the Data Object ID; b) the DateTime; c) a Type descriptor; d) the text Description of a data object and any appropriate coding; e) a Status descriptor; f) the Source; g) any Link; h) any Reference; i) any Comment,  to simplify their reiterative use in the CCR .xsd.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CCRDataObjectID" type="xs:ID">
				<xs:annotation>
					<xs:documentation>The is the unique object ID of this data object and is required.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DateTime" type="DateTimeType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Type" minOccurs="0"/>
			<xs:element ref="Description" minOccurs="0"/>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:group ref="SLRCGroup"/>
		</xs:sequence>
	</xs:complexType>
	<xs:element name="Directions">
		<xs:annotation>
			<xs:documentation>This is the SIG or administration instruction portion of a prescription or order, or the Directions for use of any product.
            </xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="Description" minOccurs="0"/>
				<xs:element name="Dose" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="MeasureType">
								<xs:sequence>
									<xs:element name="DoseSequencePosition" type="xs:integer" minOccurs="0"/>
									<xs:element name="VariableDoseModifier" type="CodedDescriptionType" minOccurs="0">
										<xs:annotation>
											<xs:documentation>This is a text Modifier for a variable dose and is Optional.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
							</xs:extension>
						</xs:complexContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="DoseCalculation" minOccurs="0" maxOccurs="unbounded">
					<xs:complexType>
						<xs:complexContent>
							<xs:extension base="DoseCalculationType">
								<xs:sequence>
