<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2005 rel. 3 U (http://www.altova.com) by Rick Peters (American Academy of Family Physicians) -->
<xs:schema xmlns="urn:astm-org:CCR" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ccr="urn:astm-org:CCR" targetNamespace="urn:astm-org:CCR" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="ContinuityOfCareRecord">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CCRDocumentObjectID" type="xs:string"/>
				<xs:element name="DateTime" type="ISO8601DateTimeSeconds">
					<xs:annotation>
						<xs:documentation>This is the CCR Creation date time. It must be expressed with precision to the second.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Patient" maxOccurs="2">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ActorID" type="xs:IDREF"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="From" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ActorLink" type="ActorReferenceType" maxOccurs="unbounded"/>
							<xs:element name="Actor" type="ActorType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="To" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ActorLink" type="ActorReferenceType" maxOccurs="unbounded"/>
							<xs:element name="Actor" type="ActorType" minOccurs="0" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Purpose" type="PurposeType" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="Body">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Insurance" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Payer" type="InsuranceType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="AdvanceDirectives" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AdvanceDirective" type="CCRCodedDataObjectType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Support" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="SupportProvider" type="ActorReferenceType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="FunctionalStatus" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Function" type="FunctionType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Problems" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Problem" type="ProblemType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="FamilyHistory" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="History" type="HistoryType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="SocialHistory" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="History" maxOccurs="unbounded">
											<xs:complexType>
												<xs:complexContent>
													<xs:extension base="CCRCodedDataObjectType">
														<xs:sequence>
															<xs:element name="Episodes" type="EpisodesType" minOccurs="0"/>
														</xs:sequence>
													</xs:extension>
												</xs:complexContent>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Alerts" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Alert" type="AlertType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Medications" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Medication" type="StructuredProductType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="MedicalEquipment" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Equipment" type="StructuredProductType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Immunizations" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Immunization" type="StructuredProductType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="VitalSigns" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Result" type="ResultType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Results" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Result" type="ResultType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Procedures" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Procedure" type="ProcedureType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="Encounters" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Encounter" type="EncounterType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="PlanOfCare" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Plan" type="PlanType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="HealthCareProviders" minOccurs="0">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Provider" type="ActorReferenceType" maxOccurs="unbounded"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Actors">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Actor" type="ActorType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="References" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Reference" type="ReferenceType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Comments" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Comment" type="CommentType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Signatures" minOccurs="0">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CCRSignature" type="SignatureType" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!--CCR Complex Types-->
	<xs:complexType name="PurposeType">
		<xs:sequence>
			<xs:element name="DateTime" type="DateTimeType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Description" maxOccurs="unbounded"/>
			<xs:element name="OrderRequest" type="PlanOfCareType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Indication" type="IndicationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Reference" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Comment" minOccurs="0" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="InsuranceType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="PaymentProvider" type="ActorReferenceType" minOccurs="0"/>
					<xs:element name="Subscriber" type="ActorReferenceType" minOccurs="0"/>
					<xs:element ref="IDs" minOccurs="0"/>
					<xs:element name="Authorizations" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Authorization" type="AuthorizationType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="FunctionType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Problem" type="ProblemType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Result" type="ResultType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HistoryType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Problem" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element ref="Type" minOccurs="0"/>
								<xs:element ref="Description" minOccurs="0"/>
								<xs:group ref="SLRCGroup"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="FamilyMember" minOccurs="0" maxOccurs="unbounded">
						<xs:complexType>
							<xs:complexContent>
								<xs:extension base="ActorReferenceType">
									<xs:sequence>
										<xs:element name="Episodes" type="EpisodesType" minOccurs="0"/>
										<xs:element name="HealthStatus" type="CurrentHealthStatusType" minOccurs="0"/>
										<xs:group ref="SLRCGroup"/>
									</xs:sequence>
								</xs:extension>
							</xs:complexContent>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ProblemType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Episodes" type="EpisodesType" minOccurs="0"/>
					<xs:element name="HealthStatus" type="CurrentHealthStatusType" minOccurs="0"/>
					<xs:element name="PatientKnowledge" type="PatientKnowledgeType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AlertType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Agent" type="AgentType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Reaction" type="ReactionType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ActorType">
		<xs:sequence>
			<xs:element name="ActorObjectID" type="xs:ID"/>
			<xs:choice>
				<xs:element name="Person">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="BirthName" type="PersonNameType" minOccurs="0"/>
										<xs:element name="FormerName" type="PersonNameType" minOccurs="0" maxOccurs="unbounded"/>
										<xs:element name="CurrentName" type="PersonNameType" minOccurs="0"/>
										<xs:element name="DisplayName" type="xs:string" minOccurs="0"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="DateOfBirth" type="DateTimeType" minOccurs="0"/>
							<xs:element name="Gender" type="GenderEnum" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Organization">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="InformationSystem">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Name" type="xs:string"/>
							<xs:element name="Type" type="xs:string" minOccurs="0"/>
							<xs:element name="Version" type="xs:string" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="IDs" type="IDType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Relation" type="CodedDescriptionType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Specialty" type="CodedDescriptionType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Address" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType mixed="true">
					<xs:sequence>
						<xs:element ref="Type" minOccurs="0"/>
						<xs:element name="Line1" type="xs:string" minOccurs="0"/>
						<xs:element name="Line2" type="xs:string" minOccurs="0"/>
						<xs:element name="City" type="xs:string" minOccurs="0"/>
						<xs:element name="County" type="xs:string" minOccurs="0"/>
						<xs:element name="State" type="xs:string" minOccurs="0"/>
						<xs:element name="Country" type="xs:string" minOccurs="0"/>
						<xs:element name="PostalCode" type="xs:string" minOccurs="0"/>
						<xs:element name="Priority" type="xs:string" minOccurs="0"/>
						<xs:element ref="Status" minOccurs="0"/>
					</xs:sequence>
					<xs:attribute name="Preferred" type="xs:string"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="Telephone" type="CommunicationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="EMail" type="CommunicationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="URL" type="CommunicationType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Status" minOccurs="0"/>
			<xs:group ref="SLRCGroup"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="StructuredProductType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Product" maxOccurs="unbounded">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ProductName" type="CodedDescriptionType"/>
								<xs:element name="BrandName" type="CodedDescriptionType" minOccurs="0"/>
								<xs:element name="Strength" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="MeasureType">
												<xs:sequence>
													<xs:element name="StrengthSequencePosition" type="xs:integer" minOccurs="0"/>
													<xs:element name="VariableStrengthModifier" type="CodedDescriptionType" minOccurs="0"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="Form" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="CodedDescriptionType">
												<xs:sequence>
													<xs:element name="FormSequencePosition" type="xs:integer" minOccurs="0"/>
													<xs:element name="MultipleFormModifier" type="CodedDescriptionType" minOccurs="0"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="Concentration" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="MeasureType">
												<xs:sequence>
													<xs:element name="ConcentrationSequencePosition" type="xs:integer" minOccurs="0"/>
													<xs:element name="VariableConcentrationModifier" type="CodedDescriptionType" minOccurs="0"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="Size" minOccurs="0" maxOccurs="unbounded">
									<xs:complexType>
										<xs:complexContent>
											<xs:extension base="CodedDescriptionType">
												<xs:sequence>
													<xs:element name="Dimensions" minOccurs="0">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Dimension" maxOccurs="unbounded">
																	<xs:complexType>
																		<xs:complexContent>
																			<xs:extension base="MeasureType">
																				<xs:sequence>
																					<xs:element ref="Description"/>
																				</xs:sequence>
																			</xs:extension>
																		</xs:complexContent>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="SizeSequencePosition" type="xs:integer" minOccurs="0"/>
													<xs:element name="VariableSizeModifier" type="CodedDescriptionType" minOccurs="0"/>
												</xs:sequence>
											</xs:extension>
										</xs:complexContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="Manufacturer" type="ActorReferenceType" minOccurs="0"/>
								<xs:element name="IDNumber" type="IDType" minOccurs="0" maxOccurs="unbounded"/>
								<xs:element name="ProductSequencePosition" type="xs:integer" minOccurs="0"/>
								<xs:element name="MultipleProductModifier" type="CodedDescriptionType" minOccurs="0"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<!-- Dosing/ordering information -->
					<xs:element name="Quantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Directions" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="PatientInstructions" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This tag is used for instructions to the patient  seperate from the "SIG", such as 'Take With Food".
                            </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Instruction" type="InstructionType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="FulfillmentInstructions" minOccurs="0">
						<xs:annotation>
							<xs:documentation>This tag is used for dispensing or fulfillment instructions to the pharmacist, such as 'Label in Spanish', 'Generic Substition OK', 'Dispense As Written', or the the Lab, such as 'Save Specimen', 'Freeze Specimen', etc.
                            </xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Instruction" type="InstructionType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Refills" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Refill" maxOccurs="unbounded">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="Number" maxOccurs="unbounded">
												<xs:simpleType>
													<xs:restriction base="xs:integer">
														<xs:minInclusive value="0"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:element>
											<xs:element name="Quantity" type="QuantityType" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element ref="Status" minOccurs="0"/>
											<xs:element name="DateTime" type="ExactDateTimeType" minOccurs="0" maxOccurs="unbounded"/>
											<xs:element name="Comment" type="CommentType" minOccurs="0" maxOccurs="unbounded"/>
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="SeriesNumber" type="xs:string" minOccurs="0"/>
					<xs:element name="Consent" type="CCRCodedDataObjectType" minOccurs="0"/>
					<xs:element name="FulfillmentHistory" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Fulfillment" type="OrderRxHistoryType" minOccurs="0" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResultType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Procedure" type="ProcedureType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Substance" type="CodedDescriptionType" minOccurs="0"/>
					<xs:element name="Test" type="TestType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TestType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element name="Method" type="CodedDescriptionType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Agent" type="AgentType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="TestResult" type="TestResultType"/>
					<xs:element name="NormalResult" minOccurs="0">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Normal" type="NormalType" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Flag" type="CodedDescriptionType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ConfidenceValue" type="CodedDescriptionType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EncounterType">
		<xs:complexContent>
			<xs:extension base="CCRCodedDataObjectType">
				<xs:sequence>
					<xs:element ref="Locations" minOccurs="0"/>
					<xs:element ref="Practitioners" minOccurs="0"/>
					<xs:element name="Frequency" type="FrequencyType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Interval" type="IntervalType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Duration" type="DurationType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Indication" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Instructions" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
