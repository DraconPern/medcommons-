	<!--Groups Used Repeatedly in the CCR-->
	<xs:group name="SLRCGroup">
		<xs:annotation>
			<xs:documentation>This is a Group that combines Source, InternalCCRLink, Reference, Comment, and Signature to simplify their reiterative use in the CCR .xsd.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Source" type="SourceType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>This defines the Source for the data object - which defines whom/what the data are from. Source is required.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="InternalCCRLink" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Reference" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element ref="Comment" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="Signature" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="SignatureID" type="xs:IDREF">
							<xs:annotation>
								<xs:documentation>This is an xs:IDREF to the SignatureObjectID in the Signatures Section of the CCR.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
