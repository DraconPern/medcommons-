        }
      }

      var is_closed = false;
      function closeSession() {
        if(is_closed)
          return;
        is_closed = true;
        
        if(window.opened) {
          if(window.name == &apos;ccr&apos;) {
            
            var context = window.location.pathname.match(/(\/[A-Za-z]*)\/.*$/)[1];
            var expires = new Date( new Date().getTime()-100000 );
