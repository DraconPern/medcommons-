<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="net.medcommons.modules.backup"> 
<class name="net.medcommons.modules.backup.BackupQueueEntry" table="backup_queue">
  <id name="id" column="id"> 
       <generator class="native"/> 
  </id> 
  <property name="queuetime" column="queuetime"   />
  <property name="starttime" column="starttime"   />
  <property name="endtime" column="endtime"   />
  <property name="accountId" column="account_id"   />
  <property name="guid" column="guid"   />
  <property name="status" column="status"   />
  <property name="size" column="size"   />
 </class>
</hibernate-mapping>
