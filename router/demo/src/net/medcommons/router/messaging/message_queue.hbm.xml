<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping package="net.medcommons.router.messaging"> 
<class name="net.medcommons.router.messaging.MessageQueue" table="message_queue">
  <id name="id" column="id"> 
       <generator class="native"/> 
  </id> 
  <property name="queuetime" column="queuetime"   />
  <property name="connectionId" column="connection_id"/>
  <property name="message" length="2048"/>
  <property name="source"/>
 </class>
</hibernate-mapping>
