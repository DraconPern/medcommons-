<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <servlet>
    <servlet-name>PostMirrorServlet</servlet-name>
    <servlet-class>net.medcommons.central.ws.test.PostMirrorServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet>
  	<servlet-name>OrderProcessorServlet</servlet-name>
  	<servlet-class>net.medcommons.central.ws.order.OrderProcessorServlet</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>PostMirrorServlet</servlet-name>
    <url-pattern>/PostMirrorServlet</url-pattern>
  </servlet-mapping>  
  
  <servlet-mapping>
  	<servlet-name>OrderProcessorServlet</servlet-name>
  	<url-pattern>/OrderProcessorServlet</url-pattern>
  </servlet-mapping>
</web-app>